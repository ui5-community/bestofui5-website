{
  "_version": "1.12.0",

  "sap.app": {
    "id": "org.openui5.ui5community",
    "type": "application",
    "title": "{{app_title}}",
    "description": "{{app_description}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": ["en"],
      "fallbackLocale": "en"
    },
    "dataSources": {
      "data": {
        "uri": "model/data.json",
        "type": "JSON"
      },
      "versions": {
        "uri": "model/versions.json",
        "type": "JSON"
      }
    }
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": {},
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "org.openui5.ui5community.view.App",
      "type": "XML",
      "async": true,
      "id": "rootView"
    },

    "dependencies": {
      "minUI5Version": "1.101.0",
      "libs": {
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.ui.unified": {},
        "sap.m": {},
        "sap.ui.webc.main": {}
      }
    },

    "handleValidation": true,

    "contentDensities": {
      "compact": true,
      "cozy": true
    },

    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },

    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "org.openui5.ui5community.i18n.i18n",
          "supportedLocales": ["en"],
          "fallbackLocale": "en"
        }
      },
      "data": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "data"
      },
      "versions": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "versions"
      }
    },

    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "org.openui5.ui5community.view",
        "controlId": "pageContainer",
        "transition": "show",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "name": "default",
          "pattern": "",
          "target": "TargetHotPackagesView"
        },
        {
          "name": "hotPackages",
          "pattern": "",
          "target": "TargetHotPackagesView"
        },
        {
          "name": "allPackages",
          "pattern": "packages:?query:",
          "target": "TargetAllPackagesView"
        },
        {
          "name": "tags",
          "pattern": "tags",
          "target": "TargetTagsView"
        },
        {
          "name": "RouteObjectView",
          "pattern": "packages/{name}",
          "target": ["TargetObjectView"]
        },
        {
          "name": "timeline",
          "pattern": "timeline",
          "target": ["TargetTimelineView"]
        }
      ],
      "targets": {
        "TargetAllPackagesView": {
          "viewType": "XML",
          "viewLevel": 1,
          "viewId": "AllPackages",
          "viewName": "AllPackages"
        },
        "TargetHotPackagesView": {
          "viewType": "XML",
          "viewLevel": 1,
          "viewId": "HotPackages",
          "viewName": "HotPackages"
        },
        "TargetTagsView": {
          "viewType": "XML",
          "viewLevel": 1,
          "viewId": "Tags",
          "viewName": "Tags"
        },
        "TargetObjectView": {
          "viewType": "XML",
          "transition": "slide",
          "viewLevel": 2,
          "viewId": "Object",
          "viewName": "Object"
        },
        "TargetTimelineView": {
          "viewType": "XML",
          "transition": "slide",
          "viewLevel": 2,
          "viewId": "Timeline",
          "viewName": "Timeline"
        }
      }
    }
  }
}
