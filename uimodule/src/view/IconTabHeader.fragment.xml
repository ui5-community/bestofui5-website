<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" >
   <ToolbarSpacer/>
   <IconTabHeader mode="Inline" selectedKey="hot" select=".onTabHeaderselect">
       <items>
           <IconTabFilter key="hot" text="Hot Packages" tooltip="Downloads last 30 Days" />
           <IconTabFilter key="recently" text="Recently Created"/>
           <IconTabFilter key="all" text="All Packages" />
           <IconTabFilter key="tags" text="All Tags"/>
       </items>
   </IconTabHeader>
   <ToolbarSpacer/>
   <Button 
  core:require="{ sapMLib: 'sap/m/library' }" 
  text="Add your own package"
  type="Emphasized"
  icon="sap-icon://add-product"
  press="sapMLib.URLHelper.redirect('https://github.com/ui5-community/ui5-community.github.io/issues/new?assignees=&amp;labels=new%20package&amp;template=new_package.md&amp;titel=Add%20new%20package', true)"
/>
</core:FragmentDefinition>