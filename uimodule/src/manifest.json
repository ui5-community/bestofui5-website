{
  "_version": "1.12.0",

  "sap.app": {
    "id": "com.sap.ui5community",
    "type": "application",
    "title": "{{app_title}}",
    "description": "{{app_description}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": ["en"],
      "fallbackLocale": "en"
    },
    "dataSources": {
      "data": {
        "uri": "model/data.json",
        "type": "JSON"
      }
    }
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": {},
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "flexEnabled": true,
    "rootView": {
      "viewName": "com.sap.ui5community.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },

    "dependencies": {
      "minUI5Version": "1.100.0",
      "libs": {
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.ui.unified": {},
        "sap.m": {}
      }
    },

    "handleValidation": true,

    "contentDensities": {
      "compact": true,
      "cozy": true
    },

    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },

    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sap.ui5community.i18n.i18n",
          "supportedLocales": ["en"],
          "fallbackLocale": "en"
        }
      },
      "data": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "data"
      }
    },

    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.sap.ui5community.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "clearControlAggregation": false,
        "async": true
      },
      "routes": [
        {
          "name": "RouteMainView",
          "pattern": "",
          "target": ["TargetMainView"]
        },
        {
          "name": "RouteObjectView",
          "pattern": "packages/{name}",
          "target": ["TargetObjectView"]
        },
        {
          "name": "RouteTagsView",
          "pattern": "tags",
          "target": ["TargetMainView"]
        }
      ],
      "targets": {
        "TargetMainView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "TargetObjectView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Object",
          "viewName": "Object"
        }
      }
    }
  }
}
