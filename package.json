{
  "name": "ui5community",
  "version": "1.0.0",
  "description": "UI5 Application: ui5community",
  "author": "marianfoo",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm-run-all build:ts build:ui5",
    "build:opt": "npm-run-all build:ts build:ui5:opt",
    "build:ts": "babel uimodule/src --out-dir uimodule/webapp --source-maps true --extensions \".ts,.js\" --copy-files",
    "build:ui5": "ui5 build --clean-dest",
    "build:ui5:opt": "ui5 build self-contained --clean-dest --all",
    "start": "npm-run-all --parallel watch:ts start:ui5",
    "watch:ts": "babel uimodule/src --out-dir webapp --source-maps true --extensions \".ts,.js\" --copy-files --watch",
    "watch:controls": "npx @ui5/ts-interface-generator",
    "start:ui5": "ui5 serve --port 8080 -o index.html --config uimodule/ui5.yaml",
    "start:dist": "ui5 serve  --port 8080 -o index.html --config uimodule/ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "lint": "eslint uimodule/src"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@openui5/ts-types-esm": "1.100.0",
    "@types/jquery": "3.5.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@ui5/cli": "^2.14.1",
    "@ui5/ts-interface-generator": "^0.4.1",
    "babel-preset-transform-ui5": "^7.0.5",
    "eslint": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.3",
    "ui5-middleware-livereload": "^0.5.8"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "ui5-cc-md"
    ]
  },
  "dependencies": {
    "ui5-cc-md": "^0.0.4",
    "xml-formatter": "^2.6.1"
  }
}
